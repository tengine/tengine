# encoding: utf-8
前提 /^以下の<%= plural_name_ja %>が登録されている$/ do |<%= plural_name %>|
  Given %{the following <%= plural_name %>:}, <%= plural_name %>
end

もし /^"<%= singular_name_ja %>一覧画面"で(\d+)番目の<%= singular_name_ja %>を削除する$/ do |pos|
  When %{I delete the #{pos}th <%= singular_name %>}
end

ならば /^以下の<%= plural_name_ja %>の一覧が表示されること$/ do |expected_table|
  Then %{I should see the following <%= plural_name %>:}, expected_table
end

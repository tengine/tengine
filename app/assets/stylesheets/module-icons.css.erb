}
.BtnPreview {
    padding: 0 5px 0 18px;
  background: url(<%= asset_path("icon/Text_preview.png") %>) no-repeat 0 2px;
}

img.BtnSample {
  float: left;
  vertical-align: top;
  margin-top: -1px;
  margin-left: 5px;
  margin-right: 5px;
}

input.BtnSample,
a.BtnText {
  display: inline-block;
  vertical-align: bottom;
  margin: 0 5px;
  padding:11px 15px 11px 30px;  
  /*
  background: url(<%= asset_path("icon/1.png") %>) no-repeat 15px center, url(<%= asset_path("bg_btn_input.png") %>) repeat-x;
  */
  
  background: url(<%= asset_path("icon/View.png") %>) no-repeat 11px center,
  -moz-linear-gradient(top, #fff,#fff 30%,#EDEDED); /* Firefox用 */
    
    background: url(<%= asset_path("icon/View.png") %>) no-repeat 11px center,
    -webkit-gradient(linear, left top, left bottom, from(#fff), 
            color-stop(0.3, #fff),
            to(#EDEDED)); /* Safari,Google Chrome用 */
    
  border: 1px solid #bbb;
  border-right-color: #aaa;
  border-bottom-color: #aaa;
  border-radius:8px;
  /*
  -webkit-border-radius:8px;
  -moz-border-radius:8px;
  */
  box-shadow: 0px 0px 5px #ccc;
  
  line-height: 1;
  color: #333;
  font-size:17px;
  font-weight: normal;
  text-align: center;
  text-decoration:none;
}

input.BtnSample {
  line-height: 0.95;
}


input.BtnSample:hover,
a.BtnText:hover {
  background: url(<%= asset_path("icon/View.png") %>) no-repeat 11px center,
  -moz-linear-gradient(top, #EAEAEA,#EAEAEA 30%,#c9c9c9); /* Firefox用 */
    
    background: url(<%= asset_path("icon/View.png") %>) no-repeat 11px center,
    -webkit-gradient(linear, left top, left bottom, from(#EAEAEA), 
            color-stop(0.3, #EAEAEA),
            to(#c9c9c9)); /* Safari,Google Chrome用 */
    
  border-color: #999;
  border-right-color: #888;
  border-bottom-color: #888;
  
  
  box-shadow: 0px 0px 5px #aaa;
  color: #000;
}



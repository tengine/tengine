}
.BtnBack {
    background-image: url(<%= asset_path("icon/Previous.png") %>);
}
.BtnNext {
    background-image: url(<%= asset_path("icon/Next.png") %>);
}
.BtnSearch {
    background-image: url(<%= asset_path("icon/View.png") %>);
}
.BtnDownload {
    background-image: url(<%= asset_path("icon/Download.png") %>);
}
.BtnUpload {
    background-image: url(<%= asset_path("icon/Upload.png") %>);
}
.BtnSubmit {
  background-image: url(<%= asset_path("icon/Go_forward.png") %>);
}
.BtnYes {
    background-image: url(<%= asset_path("icon/yes.png") %>);
}
.BtnNo {

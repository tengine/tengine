/* @group Btn (Link,Input) */
.BtnWrap {
  display: inline-block;
  margin-right: 5px;
  margin-bottom: 10px;
  padding: 0 18px 0 20px;
  border: 1px solid #ccc;
  border-right-color: #bbb;
  border-bottom-color: #aaa;
  border-radius:8px;
  -webkit-border-radius:8px;
  -moz-border-radius:8px;

  line-height: 1;
  font-weight: bold;
  background: #fff url(<%= asset_path("bg_btn_input.png") %>) repeat-x left 0;
  
  box-shadow: 0px 0px 5px #ccc;
}

.BtnWrap:hover {
  background-position: left bottom;
  border-color: #aaa;
  box-shadow: 0px 0px 0 #fff;
}
  a.BtnWrap span,
  span.BtnWrap input {
    display: inline-block;
    margin:0;
      padding: 7px 0 7px 20px;
    border: 0;
    line-height: 1;
    color: #555;
    font-size:18px;
    text-align: center;
    background-color: transparent;
    background-repeat: no-repeat;
    background-position: left center;
  }
  a.BtnWrap:hover span,
  span.BtnWrap:hover input {
    color: #222;
  }

.BtnWrap .BtnNormal {
  padding: 7px 20px;
}

.BtnDetail {
    background-image: url(<%= asset_path("icon/Text_preview.png") %>);
}
.BtnAdd {
    background-image: url(<%= asset_path("icon/Create.png") %>);
}
.BtnEdit {
    background-image: url(<%= asset_path("icon/Notes.png") %>);
}
.BtnDelete {
    background-image: url(<%= asset_path("icon/33.png") %>);
}
.BtnCancel {
    background-image: url(<%= asset_path("icon/Cancel.png") %>);
}
.BtnSetting {
    background-image: url(<%= asset_path("icon/Application.png") %>);
}
.BtnSave {

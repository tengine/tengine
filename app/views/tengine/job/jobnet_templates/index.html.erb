<h1><%= page_title Tengine::Job::JobnetTemplate, :list %></h1>

<p id="notice"><%= notice %></p>

<table>
  <tr>
    <th><%= Tengine::Job::JobnetTemplate.human_attribute_name(:name) %></th>
    <th><%= Tengine::Job::JobnetTemplate.human_attribute_name(:server_name) %></th>
    <th><%= Tengine::Job::JobnetTemplate.human_attribute_name(:credential_name) %></th>
    <th><%= Tengine::Job::JobnetTemplate.human_attribute_name(:killing_signals) %></th>
    <th><%= Tengine::Job::JobnetTemplate.human_attribute_name(:killing_signal_interval) %></th>
    <th><%= Tengine::Job::JobnetTemplate.human_attribute_name(:description) %></th>
    <th><%= Tengine::Job::JobnetTemplate.human_attribute_name(:dsl_version) %></th>
    <th><%= Tengine::Job::JobnetTemplate.human_attribute_name(:lock_version) %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobnet_templates.each do |jobnet_template| %>
  <tr class="<%= cycle("even", "odd") %>" >
    <td><%= jobnet_template.name %></td>
    <td><%= jobnet_template.server_name %></td>
    <td><%= jobnet_template.credential_name %></td>
    <td><%= jobnet_template.killing_signals_text %></td>
    <td><%= jobnet_template.killing_signal_interval %></td>
    <td><%= jobnet_template.description %></td>
    <td><%= jobnet_template.dsl_version %></td>
    <td><%= jobnet_template.lock_version %></td>
    <td><%= link_to_show jobnet_template %></td>
    <td><%= link_to_edit edit_tengine_job_jobnet_template_path(jobnet_template) %></td>
    <td><%= link_to_destroy jobnet_template, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= paginate @jobnet_templates %>

<br />

<%= link_to_new Tengine::Job::JobnetTemplate, new_tengine_job_jobnet_template_path %>

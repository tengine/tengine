<h1><%= page_title Tengine::Job::Jobnet, :list %></h1>

<p id="notice"><%= notice %></p>

<table>
  <tr>
    <th><%= Tengine::Job::Jobnet.human_attribute_name(:name) %></th>
    <th><%= Tengine::Job::Jobnet.human_attribute_name(:server_name) %></th>
    <th><%= Tengine::Job::Jobnet.human_attribute_name(:credential_name) %></th>
    <th><%= Tengine::Job::Jobnet.human_attribute_name(:killing_signals) %></th>
    <th><%= Tengine::Job::Jobnet.human_attribute_name(:killing_signal_interval) %></th>
    <th><%= Tengine::Job::Jobnet.human_attribute_name(:description) %></th>
    <th><%= Tengine::Job::Jobnet.human_attribute_name(:script) %></th>
    <th><%= Tengine::Job::Jobnet.human_attribute_name(:jobnet_type_cd) %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobnets.each do |jobnet| %>
  <tr class="<%= cycle("even", "odd") %>" >
    <td><%= jobnet.name %></td>
    <td><%= jobnet.server_name %></td>
    <td><%= jobnet.credential_name %></td>
    <td><%= jobnet.killing_signals_text %></td>
    <td><%= jobnet.killing_signal_interval %></td>
    <td><%= jobnet.description %></td>
    <td><%= jobnet.script %></td>
    <td><%= jobnet.jobnet_type_cd %></td>
    <td><%= link_to_show jobnet %></td>
    <td><%= link_to_edit edit_tengine_job_jobnet_path(jobnet) %></td>
    <td><%= link_to_destroy jobnet, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= paginate @jobnets %>

<br />

<%= link_to_new Tengine::Job::Jobnet, new_tengine_job_jobnet_path %>

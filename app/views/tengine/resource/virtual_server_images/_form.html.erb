<div class="BoxBorder BoxSearchForm">
  <%= form_for(@virtual_server_image, as:"virtual_server_image") do |f| %>
    <%= model_error_messages(@virtual_server_image) %>
    <table class= "TableForm4rows" >
      <tbody class= "CelRow2">
        <tr>
          <th><%= f.label :name %></th>
          <td><%= f.text_field :name, class: "InputText InputW200"  %></td>
        </tr>
        <tr>
          <th><%= f.label :description %></th>
          <td><%= f.text_area :description, class: "InputText InputW200"  %></td>
        </tr>
        <tr>
          <th><%= f.label :provided_id %></th>
          <td><%= @virtual_server_image.provided_id %></td>
        </tr>
        <tr>
         <th><%= f.label :provided_description %></th>
         <td><%= @virtual_server_image.provided_description %></td>
        </tr>
      </tbody>
    </table>

    <div class="BoxBtns">
      <span class="BtnWrap">
        <%= f.submit :class => "BtnNext"  %>
      </span>
    </div>
  <% end %>
</div>

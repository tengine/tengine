<h1><%= page_title Tengine::Job::Vertex, :list %></h1>

<p id="notice"><%= notice %></p>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vertices.each do |vertex| %>
  <tr class="<%= cycle("even", "odd") %>" >
    <td><%= link_to_show vertex %></td>
    <td><%= link_to_edit edit_tengine_job_vertex_path(vertex) %></td>
    <td><%= link_to_destroy vertex, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= paginate @vertices %>

<br />


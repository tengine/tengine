<h1><%= page_title Tengine::Core::Setting, :list %></h1>

<p id="notice"><%= notice %></p>

<table>
  <tr>
    <th><%= Tengine::Core::Setting.human_attribute_name(:name) %></th>
    <th><%= Tengine::Core::Setting.human_attribute_name(:value) %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @settings.each do |setting| %>
  <tr class="<%= cycle("even", "odd") %>" >
    <td><%= setting.name %></td>
    <td><%= setting.value %></td>
    <td><%= link_to_show setting %></td>
    <td><%= link_to_edit edit_tengine_core_setting_path(setting) %></td>
    <td><%= link_to_destroy setting, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= paginate @settings %>

<br />

<%= link_to_new Tengine::Core::Setting, new_tengine_core_setting_path %>
